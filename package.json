    "jsonwebtoken": "^5.7.0",
    "mailchimp-v3": "^1.0.1",
    "mailgun-js": "^0.7.10",
    "moment": "^2.29.4",
    "mongoose": "^4.4.13",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "sendgrid": "^4.7.1",
